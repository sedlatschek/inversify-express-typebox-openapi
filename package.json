{
  "name": "inversify-express-typebox-openapi",
  "version": "0.1.0",
  "description": "Generate OpenAPI specification from inversify-express-utils controllers",
  "scripts": {
    "test": "vitest",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sedlatschek/inversify-express-typebox-openapi.git"
  },
  "keywords": [
    "OpenAPI",
    "OA2",
    "OA3",
    "Swagger",
    "Inversify",
    "Express",
    "Inversify-Express-Utils",
    "Typebox"
  ],
  "author": "Simon Sedlatschek <code@sedlatschek.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sedlatschek/inversify-express-typebox-openapi/issues"
  },
  "homepage": "https://github.com/sedlatschek/inversify-express-typebox-openapi#readme",
  "dependencies": {
    "fast-equals": "^5.0.1",
    "openapi3-ts": "^4.3.1",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.32.0",
    "inversify": "^6.0.0",
    "inversify-express-utils": "^6.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.18.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.5.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
